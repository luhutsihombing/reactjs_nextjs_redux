{
  "singleQuote": true,
  "printWidth": 100,
  "editor.formatOnSave": true,
  "proseWrap": "always",
  "tabWidth": 2,
  "indent_size": 2,
  "useTabs": false,
  "bracketSpacing": false,
  "jsxBracketSameLine": true,
  "quoteProps": "as-needed",
  "arrowParens": "avoid",
  "jsxSingleQuote": false,
  "semi": true,
  "semicolons": true,
  "endOfLine": "auto",
  "trailingComma": "none"
}
